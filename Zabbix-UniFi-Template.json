            {
                "uuid": "e77c959a1e0c4364a17b773a21cbd8b7",
                "template": "UniFi Discovery and Collection Template",
                "name": "UniFi Discovery and Collection Template",
                "description": "\u041c\u043e\u0436\u043d\u043e \u043b\u0438\u0431\u043e \u0440\u0443\u043a\u0430\u043c\u0438 \u0432 \u043c\u0430\u043a\u0440\u043e\u0441\u0430\u0445 \u0443\u043a\u0430\u0437\u0430\u0442\u044c \u0430\u0434\u0440\u0435\u0441 \u0441\u0435\u0440\u0432\u0435\u0440\u0430, \u043b\u0438\u0431\u043e \u0432 \u043f\u0440\u0430\u0432\u0438\u043b\u0435 \u043e\u0431\u043d\u0430\u0440\u0443\u0436\u0435\u043d\u0438\u044f \u0437\u0430\u043c\u0435\u043d\u0438\u0442\u044c \u043c\u0430\u043a\u0440\u043e\u0441 \u0447\u0435\u043c-\u0442\u043e \u0432\u0440\u043e\u0434\u0435 {HOST.CONN}",
                "groups": [
                    {
                        "name": "Templates"
                    }
                ],
                "items": [
                    {
                        "uuid": "eb8d5ef22bbc423ea829254408448a70",
                        "name": "UniFi Site Discover Source",
                        "type": "SCRIPT",
                        "key": "unifi.site.discover[source]",
                        "delay": "8h",
                        "trends": "0",
                        "value_type": "CHAR",
                        "params": "try {\n   var params = JSON.parse(value),\n   req = new HttpRequest(),\n   fields = {},\n   resp,\n   server = params.server,\n   port = params.port;\n\n   req.addHeader('Content-Type: application/json');\n   req.addHeader('Accept: application/json');\n   fields.username = params.username;\n   fields.password = params.password;\n\n   req.post(\"https://\" + server + \":\" + port + \"/api/login\", JSON.stringify(fields));\n   resp = req.get(\"https://\" + server + \":\" + port + \"/api/self/sites\");\n   req.post(\"https://\" + server + \":\" + port + \"/api/logout\");\n   return resp;\n//   return req.getStatus();\n}\ncatch (error) {\n   return {'error.name' : error.name, 'error.message' : error.message}\n}",
                        "preprocessing": [
                            {
                                "type": "JSONPATH",
                                "parameters": [
                                    "$.data"
                                ]
                            }
                        ],
                        "parameters": [
                            {
                                "name": "password",
                                "value": "{$UNIFI_PASSWORD}"
                            },
                            {
                                "name": "port",
                                "value": "{$UNIFI_PORT}"
                            },
                            {
                                "name": "server",
                                "value": "{$UNIFI_SERVER}"
                            },
                            {
                                "name": "username",
                                "value": "{$UNIFI_USERNAME}"
                            }
                        ],
                        "tags": [
                            {
                                "tag": "Application",
                                "value": "UniFi"
                            }
                        ]
                    }
                ],
