            <items>
                <item>
                    <uuid>ca48f16b15244a7dbe5dbb24157d17b5</uuid>
                    <name>Adopted</name>
                    <type>TRAP</type>
                    <key>unifi.data[adopted]</key>
                    <delay>0</delay>
                    <valuemap>
                        <name>Logical</name>
                    </valuemap>
                    <preprocessing>
                        <step>
                            <type>STR_REPLACE</type>
                            <parameters>
                                <parameter>True</parameter>
                                <parameter>1</parameter>
                            </parameters>
                        </step>
                        <step>
                            <type>STR_REPLACE</type>
                            <parameters>
                                <parameter>False</parameter>
                                <parameter>0</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <uuid>113f71636f0b49f2bcc2e7ae2ab3c3d6</uuid>
                            <expression>last(/UniFi Data Template/unifi.data[adopted])=0</expression>
                            <name>Device not Adopted</name>
                            <opdata>Adopt status - {ITEM.LASTVALUE}</opdata>
                            <priority>INFO</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <uuid>b8e64fc643534b248594f26798ed4862</uuid>
                    <name>Device type</name>
