        <template>
            <uuid>e77c959a1e0c4364a17b773a21cbd8b7</uuid>
            <template>UniFi Discovery and Collection Template</template>
            <name>UniFi Discovery and Collection Template</name>
            <description>Можно либо руками в макросах указать адрес сервера, либо в правиле обнаружения заменить макрос чем-то вроде {HOST.CONN}</description>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <items>
                <item>
                    <uuid>eb8d5ef22bbc423ea829254408448a70</uuid>
                    <name>UniFi Site Discover Source</name>
                    <type>SCRIPT</type>
                    <key>unifi.site.discover[source]</key>
                    <delay>8h</delay>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <params>try {
   var params = JSON.parse(value),
   req = new HttpRequest(),
   fields = {},
   resp,
   server = params.server,
   port = params.port;

   req.addHeader('Content-Type: application/json');
   req.addHeader('Accept: application/json');
   fields.username = params.username;
   fields.password = params.password;

   req.post(&quot;https://&quot; + server + &quot;:&quot; + port + &quot;/api/login&quot;, JSON.stringify(fields));
   resp = req.get(&quot;https://&quot; + server + &quot;:&quot; + port + &quot;/api/self/sites&quot;);
   req.post(&quot;https://&quot; + server + &quot;:&quot; + port + &quot;/api/logout&quot;);
   return resp;
//   return req.getStatus();
}
catch (error) {
   return {'error.name' : error.name, 'error.message' : error.message}
}</params>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.data</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <parameters>
                        <parameter>
                            <name>password</name>
                            <value>{$UNIFI_PASSWORD}</value>
                        </parameter>
                        <parameter>
                            <name>port</name>
                            <value>{$UNIFI_PORT}</value>
                        </parameter>
                        <parameter>
                            <name>server</name>
                            <value>{$UNIFI_SERVER}</value>
                        </parameter>
                        <parameter>
                            <name>username</name>
                            <value>{$UNIFI_USERNAME}</value>
                        </parameter>
                    </parameters>
                    <tags>
                        <tag>
                            <tag>Application</tag>
                            <value>UniFi</value>
                        </tag>
                    </tags>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <uuid>4c9a1113c66d4ae99566b98097cce7ce</uuid>
                    <name>Unifi Devices Discover</name>
                    <key>unifi.device.discover[{$UNIFI_SERVER},{$UNIFI_PORT},{$UNIFI_USERNAME},{$UNIFI_PASSWORD}]</key>
                    <delay>8h</delay>
                    <lifetime>7d</lifetime>
                    <host_prototypes>
                        <host_prototype>
                            <uuid>be151c3e28e84e499ded0c65e909e1d4</uuid>
                            <host>UniFi_{#DEVICEID}</host>
                            <name>{#DEVICENAME} ({#DEVICEMAC})</name>
                            <group_links>
                                <group_link>
                                    <group>
                                        <name>UniFi Device Discovery</name>
                                    </group>
                                </group_link>
                            </group_links>
                            <group_prototypes>
                                <group_prototype>
                                    <name>{#SITEDESC}</name>
                                </group_prototype>
                            </group_prototypes>
                            <templates>
                                <template>
                                    <name>UniFi Data Template</name>
                                </template>
                            </templates>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>UniFi</value>
                                </tag>
                            </tags>
                            <custom_interfaces>YES</custom_interfaces>
                            <interfaces>
                                <interface>
                                    <type>SNMP</type>
                                    <ip>{#DEVICEIP}</ip>
                                    <port>161</port>
                                    <details>
                                        <community>{$SNMP_COMMUNITY}</community>
                                    </details>
                                </interface>
                            </interfaces>
                            <inventory_mode>AUTOMATIC</inventory_mode>
                        </host_prototype>
                    </host_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <uuid>a13337994ccb46d3bc223ac9286f065a</uuid>
                    <name>UniFi Site Discover</name>
                    <type>DEPENDENT</type>
                    <key>unifi.site.discover</key>
                    <delay>0</delay>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <uuid>bb0ea80b19d0431e825053c9867a08e1</uuid>
                            <name>Unifi Site - {#SITE_DESC}</name>
                            <key>unifi.device.data[{$UNIFI_SERVER},{$UNIFI_PORT},{$UNIFI_USERNAME},{$UNIFI_PASSWORD},{#SITE_NAME}]</key>
                            <delay>5m</delay>
                            <history>7d</history>
                            <units>devices</units>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>UniFi</value>
                                </tag>
                            </tags>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>unifi.site.discover[source]</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#SITE_DESC}</lld_macro>
                            <path>$.desc</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#SITE_NAME}</lld_macro>
                            <path>$.name</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$UNIFI_PASSWORD}</macro>
                    <type>SECRET_TEXT</type>
                </macro>
                <macro>
                    <macro>{$UNIFI_PORT}</macro>
                    <value>8443</value>
                </macro>
                <macro>
                    <macro>{$UNIFI_SERVER}</macro>
                    <value>127.0.0.1</value>
                </macro>
                <macro>
                    <macro>{$UNIFI_USERNAME}</macro>
                    <value>root</value>
                </macro>
            </macros>
        </template>
