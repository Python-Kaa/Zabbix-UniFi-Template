                            <name>Device in EOL</name>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <uuid>3db530b2abd44815b0a31068f27f575b</uuid>
                    <name>Satisfaction</name>
                    <type>TRAP</type>
                    <key>unifi.data[satisfaction]</key>
                    <delay>0</delay>
                    <status>DISABLED</status>
