                    <delay>0</delay>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <inventory_link>TYPE</inventory_link>
                </item>
                <item>
                    <uuid>66f3d388bee946a9b6b12e4cbd03fc4a</uuid>
                    <name>Last Seen</name>
