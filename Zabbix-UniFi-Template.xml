                    <delay>0</delay>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                </item>
                <item>
                    <uuid>4f393f1db8194c70b9ed79d15094f210</uuid>
                    <name>Device State</name>
                    <type>TRAP</type>
                    <key>unifi.data[state]</key>
                    <delay>0</delay>
                    <valuemap>
                        <name>Device State</name>
                    </valuemap>
                    <triggers>
                        <trigger>
                            <uuid>43aa78e854344de6a66bfec7158b51af</uuid>
                            <expression>last(/UniFi Data Template/unifi.data[state],#2)=0</expression>
                            <name>Device disconnected</name>
                            <priority>HIGH</priority>
                        </trigger>
                        <trigger>
                            <uuid>bb127d08b9c64b40965f1ea61c64374a</uuid>
                            <expression>last(/UniFi Data Template/unifi.data[state])&gt;2</expression>
                            <name>Device not in &quot;Available&quot; state</name>
                            <opdata>{ITEM.LASTVALUE}</opdata>
                            <priority>INFO</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <uuid>88b4f308d6b54b968a760860c1924b07</uuid>
                    <name>Firmware update available</name>
                    <type>TRAP</type>
                    <key>unifi.data[upgradable]</key>
                    <delay>0</delay>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <triggers>
                        <trigger>
                            <uuid>577429e64d3f46aba7f8926d4ed9a57c</uuid>
                            <expression>find(/UniFi Data Template/unifi.data[upgradable],,&quot;like&quot;,&quot;No&quot;)=0</expression>
                            <name>Firmware upgrade available</name>
                            <opdata>{ITEM.LASTVALUE}</opdata>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <uuid>1e019fec1d0141b0b5fc0cd6d14fd0bc</uuid>
                    <name>Uptime</name>
                    <type>TRAP</type>
                    <key>unifi.data[uptime]</key>
                    <delay>0</delay>
                    <units>s</units>
                    <triggers>
                        <trigger>
                            <uuid>32efd3658395485b8012c8d615a4aa7c</uuid>
                            <expression>last(/UniFi Data Template/unifi.data[uptime])&lt;10m</expression>
                            <name>Device has been restarted</name>
                            <opdata>{ITEM.LASTVALUE}</opdata>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <uuid>a37d8b33535240f9afc667ef6be19ea3</uuid>
                    <name>Clients</name>
                    <type>TRAP</type>
                    <key>unifi.data[usercount]</key>
                    <delay>0</delay>
                </item>
